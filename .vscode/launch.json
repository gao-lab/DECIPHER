{
  "version": "0.2.0",
  "configurations": [
    {
      "python": "${command:python.interpreterPath}",
      "name": "Py:default",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${fileDirname}",
      "justMyCode": true,
      "env": {
        "HTTPS_PROXY": "http://127.0.0.1:7890",
        "HTTP_PROXY": "http://127.0.0.1:7890",
      },
      "args": [],
      "stopOnEntry": false,
    }
  ]
}
